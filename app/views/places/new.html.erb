<section class ="booyah-box col-xs-10 col-xs-offset-1">
	<h1>Enter Your Music Store Details</h1>
	<%= simple_form_for @place do |f| %>
		<%= f.input :name, label: 'Store Name' %>
		<%= f.input :address %>
		<%= f.input :description %>
		<br>
		<%= f.submit 'Create', :class => 'btn-primary' %>
	<% end %>
</section>